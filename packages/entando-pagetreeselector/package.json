{
  "name": "@entando/pagetreeselector",
  "version": "2.0.1",
  "description": "Page Tree Selector React Component Used in Entando Projects",
  "main": "dist/js/index.js",
  "scripts": {
    "test": "jest --env=jsdom",
    "lint": "eslint ./modules",
    "coverage": "jest --env=jsdom --coverage",
    "build": "babel modules -d dist/js && node-sass --output-style compressed sass/index.scss -o dist/css",
    "prepublish": "npm run build"
  },
  "repository": "https://github.com/entando/frontend-libraries",
  "keywords": [
    "page",
    "tree",
    "selector"
  ],
  "license": "LGPL-2.1",
  "files": [
    "dist",
    "!test",
    "!modules",
    "!sass"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^23.5.0",
    "jest-enzyme": "^6.0.0",
    "node-sass": "^4.9.0",
    "patternfly-react": "^1.19.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "modules",
      "test"
    ],
    "collectCoverageFrom": [
      "modules/**/*.{js}"
    ],
    "testMatch": [
      "<rootDir>/test/**/?(*.)(spec|test).{js}"
    ]
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./modules"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.12.0",
    "react-intl": "^2.4.0"
  }
}
