{
  "name": "@entando/ui",
  "version": "0.0.1",
  "license": "LGPL-2.1",
  "description": "entando ui theme for material-ui",
  "keywords": [
    "entando",
    "ui",
    "material"
  ],
  "repository": "https://github.com/entando/frontend-libraries",
  "scripts": {
    "test": "jest --env=jsdom",
    "lint": "eslint ./modules",
    "coverage": "jest --env=jsdom --coverage",
    "build": "babel modules -d dist",
    "prepublish": "npm run build"
  },
  "files": [
    "dist",
    "!test",
    "!modules"
  ],
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.10.0",
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.5.1",
    "jest": "^24.9.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "modules",
      "test"
    ],
    "collectCoverageFrom": [
      "modules/**/*.{js}"
    ]
  },
  "babel": {
    "presets": [
      "@babel/env"
    ]
  },
  "dependencies": {
    "babel-eslint": "^10.0.3",
    "chalk": "^2.4.2"
  }
}
